{
  "name": "@kayak/playwright",
  "version": "0.0.0",
  "keywords": [
    "kayak",
    "playwright",
    "web3",
    "dapp",
    "metamask",
    "testing",
    "e2e"
  ],
  "license": "MIT",
  "author": "Evgeniy Kozlov",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kayak-playwright": "./dist/bin/index.js"
  },
  "files": [
    "dist",
    "!dist/.tmp"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -d && tsc-alias",
    "lint": "eslint --no-fix --ext .js,.ts ./src",
    "watch": "concurrently \"tsc -w\" \"tsc-alias -w\""
  },
  "dependencies": {
    "@kayak/core": "workspace:*",
    "@kayak/metamask": "workspace:*",
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@kayak/eslint-config": "workspace:*",
    "@types/node": "^18.15.11",
    "eslint": "^8.37.0",
    "tsc-alias": "^1.8.5",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "@playwright/test": "^1.32.2"
  }
}
