{
  "name": "@kayak/cypress",
  "version": "0.0.0",
  "keywords": [
    "kayak",
    "cypress",
    "web3",
    "dapp",
    "metamask",
    "testing",
    "e2e"
  ],
  "license": "MIT",
  "author": "Evgeniy Kozlov",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./commands": {
      "import": "./commands.js",
      "require": "./commands.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kayak-cypress": "./dist/bin/index.js"
  },
  "files": [
    "dist",
    "commands.js"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -d && tsc-alias",
    "lint": "eslint --no-fix --ext .js,.ts ./src",
    "watch": "concurrently \"tsc -w\" \"tsc-alias -w\""
  },
  "dependencies": {
    "@kayak/core": "workspace:*",
    "@kayak/metamask": "workspace:*",
    "commander": "^9.4.1"
  },
  "devDependencies": {
    "@kayak/eslint-config": "workspace:*",
    "@types/node": "^18.11.17",
    "eslint": "^8.30.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "cypress": "^12.2.0"
  }
}
